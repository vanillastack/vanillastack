{% for user in rbac.users %}
{% for clusterrole in rbac.clusterRoles %}
---
apiVersion: rbac.authorization.k8s.io/v1
kind: ClusterRoleBinding
metadata:
  name: "mpaas-customer-binding-{{user}}-{{clusterrole}}"
roleRef:
  apiGroup: rbac.authorization.k8s.io
  kind: ClusterRole
  name: "{{clusterrole}}"
subjects:
- kind: User
  name: "{{user}}"
  apiGroup: ""
{% endfor %}
{% endfor %}
